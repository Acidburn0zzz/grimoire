sed -i  "s:QMAKE_CXXFLAGS_DEBUG += -pedantic:#QMAKE_CXXFLAGS_DEBUG += -pedantic:"  mkspecs/defs.pro  &&
sed -i  "s:QMAKE_CXXFLAGS += -pedantic:#QMAKE_CXXFLAGS += -pedantic:"              mkspecs/defs.pro  &&
sed -i  "40a\
#include <string.h>" dxflib/src/dl_writer_ascii.h  &&
sed -i  "38a\
#include <string.h>" dxflib/src/dl_writer.h  &&
sed -i  "s/(abs/(fabs/" qcadlib/src/information/rs_information.cpp  &&
sed -i  "s/ abs/ fabs/" qcadlib/src/information/rs_information.cpp  &&

sed -i  's|os << " layer address: " << (int)(e.layer) << " ";|// os << " layer address: " << (int)(e.layer) << " ";|' \
        qcadlib/src/engine/rs_entity.cpp  &&
sed -i  's|<< " address: " << (int)(&l)|// << " address: " << (int)(&l)|' \
        qcadlib/src/engine/rs_layer.cpp  &&


sed -i  "s/(abs/(fabs/" qcadactions/src/rs_actionzoompan.cpp  &&
sed -i  "s/ abs/ fabs/" qcadactions/src/rs_actionzoompan.cpp
