depends xorg-libs  &&
depends zlib      && 
depends libpng    &&
depends giflib    &&
depends tiff      &&
depends jpeg      &&
depends freetype2 "--with-freetype-src=$INSTALL_ROOT/usr/include/freetype2"  &&

optional_depends autotrace "" "" "for outlining bitmaps for conversion to fonts" &&
optional_depends potrace "" "" "for outlining bitmaps for conversion to fonts"
