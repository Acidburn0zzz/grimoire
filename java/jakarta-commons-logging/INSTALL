extract_jakarta_src  ${SPELL#*-}             \
                     ${SPELL#*-}-${VERSION}  \
                     $SOURCE_DIRECTORY/logging/dist        &&

register_jakarta_jar                                            \
  ${SPELL#*-}-${VERSION}/commons-logging-api.jar  \
  commons-logging-api.jar                                       &&

register_jakarta_jar  ${SPELL#*-}-${VERSION}/commons-logging.jar \
                      commons-logging.jar
