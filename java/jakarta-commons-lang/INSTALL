extract_jakarta_src  ${PKG_SUBPRJ}-${PKG_NAME}                    \
                     ${PKG_SUBPRJ}-${PKG_NAME}-${VERSION}         \
                     $SOURCE_DIRECTORY/${PKG_SUBPRJ}-${PKG_NAME}  &&

register_jakarta_jar                                                   \
   ${PKG_SUBPRJ}-${PKG_NAME}-${VERSION}/dist/commons-lang-2.0-dev.jar  \
   commons-lang.jar
