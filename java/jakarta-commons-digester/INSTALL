extract_jakarta_src   ${PKG_SUBPRJ}-${PKG_NAME}             \
                      ${PKG_SUBPRJ}-${PKG_NAME}-${VERSION}  \
                      $SOURCE_DIRECTORY                     &&

register_jakarta_jar                                                \
    ${PKG_SUBPRJ}-${PKG_NAME}-${VERSION}/dist/commons-digester.jar  \
    commons-digester.jar
