extract_jakarta_src   ${PKG_SUBPRJ}-${PKG_NAME}                              \
                 ${PKG_SUBPRJ}-${PKG_NAME}-${VERSION}                        \
                 $SOURCE_DIRECTORY/${PKG_SUBPRJ}-${PKG_NAME}-${VERSION}-src  &&

register_jakarta_jar  \
   ${PKG_SUBPRJ}-${PKG_NAME}-${VERSION}/dist/commons-dbcp.jar  \
   commons-dbcp.jar
