           SPELL=libkdcraw
         VERSION=4.6.80
          SOURCE=$SPELL-$VERSION.tar.bz2
     SOURCE_HASH=sha512:
      SOURCE_URL=$KDE_URL/unstable/$VERSION/src/$SOURCE
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
        WEB_SITE=http://www.kde.org
         ENTERED=20110219
