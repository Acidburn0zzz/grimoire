   SOURCE_URL[0]=http://windowmaker.org/pub/source/release/WindowMaker-${VERSION}.tar.gz
     SOURCE_HASH=sha512:259db96b44058fde21b7efe65419b7f7ae149029dcfbf5cf480f920d68f906cd15345105ab67d8539be44d6f1b30e62accf68fc009d53af8c12e047c01154a11
