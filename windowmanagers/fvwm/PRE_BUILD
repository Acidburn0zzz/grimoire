default_pre_build &&
if [[ "$FVWM_RELEASE" == "unstable" ]] && 
   [[ "$FVWM_PATCHSET" == "y" ]]; then
	cd ${SCRIPT_DIRECTORY}/patches
	for patch in *.patch; do
		cd ${SOURCE_DIRECTORY} &&
		message "${MESSAGE_COLOR}applying ${FILE_COLOR}$patch${DEFAULT_COLOR}" &&
		patch -p1 < ${SCRIPT_DIRECTORY}/patches/$patch
	done
fi
