default_pre_build                                   &&
cd $SOURCE_DIRECTORY                                &&
if [[ "$DOVECOT_EXT_LDA" == "y" ]]; then
unpack_file 3                                       &&
if [[ "$DOVECOT_MANAGESIEVE" == "y" ]]; then
verify_file 5                                       &&
zcat $SOURCE_CACHE/$SOURCE5 | patch -p1             &&
libtoolize -f                                       &&
aclocal                                             &&
automake -f --add-missing                           &&
autoreconf -f                                       &&
cd dovecot-sieve-*                                  &&
unpack_file 7
fi
fi
