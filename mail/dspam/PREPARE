config_query_list DSPAM_BRANCH "What branch do you want to use?" \
              release \
              devel \
              scm &&

if [[ $DSPAM_BRANCH == scm ]]; then
if [[ $DSPAM_SCM == y ]]; then
config_query DSPAM_SCM_AUTO \
             "Automaticaly update the spell on sorcery queue/system-update?" \
						 n
