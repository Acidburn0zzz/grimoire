           SPELL=suds
         VERSION=0.3.1
          SOURCE=$SPELL-$VERSION.tar.gz
   SOURCE_URL[0]=http://pypi.python.org/packages/source/s/$SPELL/$SOURCE
     SOURCE_HASH=sha512:408735835a57781b09cebd1091babf8b48058be51331ad5a15de88f93ca39c33f55b359c0c4d194a5966e1240ab85cc8965a7341433689e296ab9dd6df970947
