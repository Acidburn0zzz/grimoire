   SOURCE_URL[0]=http://pyyaml.org/download/${SPELL}/${SOURCE}
     SOURCE_HASH=sha512:57536fd9e5515916189c37487e0da583c8e9f79dd52dedb18342f65aefb6c063c4f141326ae7b49411d0cfdfd493bb0516f5c53737c76a69c7050a74a2834a33
