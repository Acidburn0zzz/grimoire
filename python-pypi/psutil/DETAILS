         VERSION=5.1.2
          SOURCE=$SPELL-$VERSION.tar.gz
   SOURCE_URL[0]=https://github.com/giampaolo/psutil/archive/release-${VERSION}.tar.gz
     SOURCE_HASH=sha512:89fd7b814d2fc5587a885818147635a462c5958ea5e6dce033347e5fca426f97a6fff5e0ff693e4ee29dfbd78f93ecfe41ebefa1ade8ad41687c37a8cfcf92c9
