           SPELL=gyp
if [[ "$GYP_SCM_AUTOUPDATE" == "y" ]]
then
         VERSION=$(date +%Y%m%d)
else
         VERSION=svn
fi
          SOURCE=$SPELL-svn.tar.bz2
   SOURCE_IGNORE=volatile
