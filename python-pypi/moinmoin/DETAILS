   SOURCE_URL[0]=http://static.moinmo.in/files/$SOURCE
     SOURCE_HASH=sha512:b513c3edf63ecbd5da48bea80415a322b0a7010a890cef21cc6a1e31da2edf0e4c4b359df592bff156aad48659fa0d949a710b037d178ea7da77dc6e9c562538
