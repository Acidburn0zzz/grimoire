         SOURCE2=dist-$SOURCE.asc
      SOURCE_GPG="canonical.gpg:$SOURCE2:UPSTREAM_KEY"
   SOURCE_URL[0]=http://launchpad.net/$SPELL/trunk/${VERSION:0:4}/+download/$SOURCE
