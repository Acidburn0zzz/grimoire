   SOURCE_URL[0]=https://github.com/jborg/${SPELL}/archive/$VERSION.tar.gz
     SOURCE_HASH=sha512:16139cd93bff59414cc1ce3fd68e2dd0aed4937a20bd1003880961ad2e239a1e982fd0d4f6b791165464213b88dea6ccd8e5e59d2087809be2e1bfdfd3b3186c
