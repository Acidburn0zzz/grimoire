depends xcb-proto  &&
local xcb_version=$(installed_version xcb-proto) &&
if [[ $xcb_version == 1.0 || $xcb_version == 0.9.93 ]]; then
  force_depends xcb-proto
fi &&
depends libpthread-stubs  &&
depends automake   &&
depends autoconf   &&
depends pkgconfig  &&
depends libtool    &&
depends libxslt    &&
depends libxau     &&
depends util-macros
