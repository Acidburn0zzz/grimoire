          SOURCE="${SPELL}_${VERSION}.tar.gz"
   SOURCE_URL[0]=http://ftp.videolan.org/pub/videolan/${SPELL}/${SOURCE}
     SOURCE_HASH=sha512:335bb38cf0892f2c4310033b076d51115c0b7faa9596a4f556dde5e3d1378d04f3d0055cb0bf2441eb725b7f84c246174bdc315eeb4ddde61ef9d79469f44eef
