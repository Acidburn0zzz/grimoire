           SPELL=totem-pl-parser
         VERSION=2.32.6
          BRANCH=`echo $VERSION|cut -d . -f 1,2`
          SOURCE=$SPELL-$VERSION.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=$GNOME_URL/sources/$SPELL/$BRANCH/$SOURCE
#   SOURCE_URL[1]=http://www.hadess.net/files/software/$SPELL/$SOURCE
        WEB_SITE=http://www.gnome.org/projects/totem/
      SOURCE_GPG="gurus.gpg:${SOURCE}.sig"
      LICENSE[0]=GPL
         ENTERED=20080228
        KEYWORDS="gnome player video"
           SHORT="playlist parser library"
cat << EOF
Totem-pl-parser is a library for parsing the various playlist formats used for
online audio and video streams.
EOF
