default_pre_build &&
cd "${SOURCE_DIRECTORY}" &&
sedit 's:/tracker.h:/tracker-client.h:'  src/plugins/tracker/totem-tracker-widget.h &&
sedit 's:/tracker.h:/tracker-client.h:'  src/plugins/tracker/totem-tracker-widget.c &&
if ! is_depends_enabled $SPELL gnome-doc-utils ||
   ! is_depends_enabled $SPELL gtk-doc ; then
   sedit 's/^HELP_DIR.*/HELP_DIR = /' Makefile.in
   sedit 's/data help/data/' Makefile.in
fi
