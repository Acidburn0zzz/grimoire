default_pre_build             &&
cd  $SOURCE_DIRECTORY         &&

if grep proto CVS/Repository > /dev/null; then
message "${PROBLEM_COLOR}SOURCE_URL of $SPELL has been changed.
Please remove old /var/spool/sorcery/$SPELL-cvs.tar.bz2${DEFAULT_COLOR}"  &&
return 1
fi  &&

NOCONFIGURE=ON  ./autogen.sh
