make    install                                                  &&
make    install-help                                             &&
make    install-doc                                              &&
install_config_file lynx.cfg $INSTALL_ROOT/var/lib/lynx/lynx.cfg &&
ln    -sf  ../var/lib/lynx/lynx.cfg $INSTALL_ROOT/etc/lynx.cfg   &&
rm    -f  $INSTALL_ROOT/usr/bin/lynx.old
