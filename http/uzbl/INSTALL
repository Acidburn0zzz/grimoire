if [[ $UZBL_INSTALL == uzbl-core ]]; then
make PREFIX=${INSTALL_ROOT}/usr install-uzbl-core
elif [[ $UZBL_INSTALL == uzbl-browser ]]; then
make PREFIX=${INSTALL_ROOT}/usr install-uzbl-browser
elif [[ $UZBL_INSTALL == uzbl-tabbed ]]; then
make PREFIX=${INSTALL_ROOT}/usr install-uzbl-tabbed
else
make PREFIX=${INSTALL_ROOT}/usr install
fi

