           SPELL=nrcore
if [[ $NRCORE_SCM == y ]]; then
             PKG=NRCore
if [[ $NRCORE_SCM_AUTO == y ]]; then
         VERSION=$(date +%Y%m%d)
else
         VERSION=scm
fi
          SOURCE="${PKG}-svn.tar.bz2"
