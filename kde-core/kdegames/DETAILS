           SPELL=kdegames
         VERSION=3.5.6
     SOURCE_HASH=sha512:2947feb81f0a2b2778a71ba3f8758c876cc4c3a636cf5ec3a2eee09a01aea3dd5e072fcff83ab79553ef903ac4b68a29762e56ac1b3f989fe2afe9e77d4253c9
          SOURCE=$SPELL-$VERSION.tar.bz2
SOURCE_DIRECTORY="$BUILD_DIRECTORY/$SPELL-$VERSION"
   SOURCE_URL[0]=$KDE_URL/stable/${VERSION//.0/}/src/$SOURCE
        WEB_SITE=http://www.kde.org
         ENTERED=20010922
      LICENSE[0]=GPL
        KEYWORDS="games kde"
           SHORT="Various games like mahjongg, snake, asteroids, and tetris."
