           SPELL=man-pages-de
         VERSION=0.5
          SOURCE=manpages-de-$VERSION.tar.gz
         SOURCE2=$SOURCE.asc
SOURCE_DIRECTORY=$BUILD_DIRECTORY/manpages-de-$VERSION
SOURCE_URL[0]=http://www.infodrom.org/projects/manpages-de/download/$SOURCE
  SOURCE2_URL[0]=${SOURCE_URL[0]}.asc
      SOURCE_GPG=801EA932.gpg:$SOURCE2:UPSTREAM_KEY
  SOURCE2_IGNORE=signature
        WEB_SITE=http://www.infodrom.org/projects/manpages-de/
         ENTERED=20050717
         ARCHIVE=off
      LICENSE[0]=GPL
        KEYWORDS="man doc"
           SHORT="man-pages-de contains collection of man pages in German."
cat << EOF
The manpages package contains a large collection of man pages for Linux
covering programming APIs, file formats, protocols, etc.
This spell contains the german translation of the man pages.
EOF
