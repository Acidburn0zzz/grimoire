for VV in 3.1 4.2 4.3 4.4 4.5;do
install-catalog --remove ${INSTALL_ROOT}/etc/sgml/sgml-docbook-dtd-$VV.cat \
    /usr/share/sgml/docbook/sgml-dtd/$VV/catalog                           &&
install-catalog --remove ${INSTALL_ROOT}/etc/sgml/sgml-docbook-dtd-$VV.cat \
    /etc/sgml/sgml-docbook.cat
done

