           SPELL=boinc
if [ "$BOINC_UPDATE_DAILY" == "y" ] 
then
         VERSION=$(date +%Y%m%d)
         BRANCH=trunk/boinc
else
         VERSION=5_10
         BRANCH=branches/boinc_core_release_$VERSION
fi
          SOURCE=$SPELL-svn.tar.bz2
  FORCE_DOWNLOAD=on
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-svn
   SOURCE_URL[0]=svn://boinc.berkeley.edu/svn/$BRANCH:$SPELL
   SOURCE_IGNORE=volatile
      LICENSE[0]=GPL
      PATCHLEVEL=2
        WEB_SITE=http://boinc.berkeley.edu/
         ENTERED=20010922
        KEYWORDS="utils setiathome"
           SHORT="Distributed utility for contributing to scientific research"
cat << EOF
