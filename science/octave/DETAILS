            SPELL=octave
          VERSION=3.4.0
           SOURCE=$SPELL-$VERSION.tar.bz2
      SOURCE_HASH=sha512:af418895e10bc9bc0535420ca5ae66f9cfb2c798479009a7462fcd09dbd53a54c77a95ff6077889cc65f829fffe1b6b9098e469d6daf6d20acdcc334f024d51b
