   SOURCE_URL[0]=http://members.dslextreme.com/users/billw/gkrellm/$SOURCE
     SOURCE_HASH=sha512:f3bc055c54bd18c20a59fae471e27c7f4067067525cbc351a0e9fb80a70f7da59c220f36ecabae341e73e0b0e93546edabebff896eed863b515c6d8408a5670a
