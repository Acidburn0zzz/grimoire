           SPELL=gkrellm2
         VERSION=2.3.0
          SOURCE=gkrellm-$VERSION.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/gkrellm-$VERSION
   SOURCE_URL[0]=http://members.dslextreme.com/users/billw/gkrellm/$SOURCE
     SOURCE_HASH=sha512:c95e8e8172b495501a3b9ce06bc67ff6dd5155a4753a8ec316a697c15ab92e415b665dce058817a41c95f32e5d252d9c1f74e5b4287ed9d1efcc7562873b8056
