         WEB_SITE=http://www.mozilla.org/projects/nspr/
          ENTERED=20050512
      SOURCE_HASH=sha512:26deadeb89b4ce161ea9bf04271ba7895c78cfab1fa4cd1c82b2d5f835381e4c26c9348b690e688c870c842d5465d45113f4dc44cf71ee35d0b64cdde21bcd51
