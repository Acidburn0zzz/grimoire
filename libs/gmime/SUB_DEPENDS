case $THIS_SUB_DEPENDS in
   2.2) echo "2.2.x release needed, forcing it."  &&
             GMIME_22BRANCH="y"                          ;;
   2.4) echo "2.4.x release needed, forceing it." &&
             GMIME_22BRANCH="n"                          ;;
   *)        echo "unknown sub-depends!"; return 1         ;;
esac
