           SPELL=tevent
if [[ "$TEVENT_SCM_AUTOUPDATE"  ==  "y"  ]];  then
         VERSION=$(date +%Y%m%d)
else
         VERSION=git
fi
          SOURCE=samba4-git.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/samba4-git
   SOURCE_URL[0]=git://git.samba.org/samba.git:samba4
   SOURCE_IGNORE=volatile
      LICENSE[0]=GPL
