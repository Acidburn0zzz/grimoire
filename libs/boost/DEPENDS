depends  g++                                    &&
depends  boost-jam                              &&
for BL in $BOOST_LIBS;do
  if [ $BL == python ];then
    depends python "--with-python-root"
  fi
done                                            &&
optional_depends  icu                           \
                  "-sHAVE_ICU=1"                \
                  ""                            \
                  "Unicode support"
