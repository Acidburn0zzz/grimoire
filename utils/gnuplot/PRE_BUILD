default_pre_build    &&
cd "$SOURCE_DIRECTORY" &&
# Fix build that thinks we are cross-compiling.
# Hopefully fixed in gnuplot 4.7 .
patch -Np0 < "$SPELL_DIRECTORY/crossbuild.patch" &&

# remove the true when removing the patch above
if true || [[ $GNUPLOT_BRANCH == scm ]]; then
  cd "$SOURCE_DIRECTORY" &&
  autoreconf -iv
fi

