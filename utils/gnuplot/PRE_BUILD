default_pre_build    &&
cd "$SOURCE_DIRECTORY" &&
# Fix build that thinks we are cross-compiling.
# Hopefully fixed in gnuplot 4.7 .
patch -Np0 < "$SPELL_DIRECTORY/crossbuild.patch" &&
patch -p1 < "$SPELL_DIRECTORY/gnuplot-4.6.2-texinfo-5.patch" &&

if [[ $GNUPLOT_BRANCH == scm ]]; then
  cd "$SOURCE_DIRECTORY" &&
  autoreconf -iv
fi

