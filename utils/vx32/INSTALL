cd $SOURCE_DIRECTORY/src                    &&
default_install                             &&
install 9vx/9vx /usr/bin/9vx                &&  
install -d ${INSTALL_ROOT}/usr/include/vx32 &&
install -d ${INSTALL_ROOT}/usr/lib/vx32     &&
cp -r /usr/vx32/include/* \
${INSTALL_ROOT}/usr/include/vx32/           &&
cp -r /usr/vx32/lib/* \
${INSTALL_ROOT}/usr/lib/vx32/               &&
rm -rf /usr/vx32  


     