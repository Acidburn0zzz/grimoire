echo  "/usr/bin"                                 >  conf-bin     &&
echo  "/usr/lib/bglibs"                      >  conf-bglibs  &&
echo  "/usr/lib/bglibs"                  >  conf-bgincs  &&
echo  "gcc ${CFLAGS} -I/usr/lib/bglibs"  >  conf-cc      &&
echo  "gcc $LDFLAGS -L/usr/lib/bglibs"             >  conf-ld      &&

make  programs
