   SOURCE_URL[0]=http://www.${SPELL}.org/download/${SOURCE}
     SOURCE_HASH=sha512:81db52b29a5fd2a5e14863f1cbf948b15ea68ff41a826cdf323215c2c8b0334144aaccf00cb49a9f7c195efcb5ff49fbf2b687a8bd4b604d338af6628d319b2c
