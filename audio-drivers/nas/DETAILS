           SPELL=nas
if [[ "$NAS_DEV" = "y" ]]; then
         VERSION=1.8b
else
         VERSION=1.8
fi
          SOURCE=$SPELL-$VERSION.src.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=http://nas.codebrilliance.com/nas/$SOURCE
      SOURCE_GPG="gurus.gpg:${SOURCE}.sig"
