OSS_BRANCH=${OSS_BRANCH/test/stable}  &&
config_query_list OSS_BRANCH "Which branch do you want?"  \
                  stable scm                         &&
if [[ $OSS_BRANCH == scm ]]; then
  config_query OSS_SCM_AUTO \
  "Automaticaly update the spell on sorcery queue/system-update?" \
                      "n"
fi
