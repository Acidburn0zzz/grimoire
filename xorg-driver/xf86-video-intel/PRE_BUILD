if [[ "$(get_kernel_config CONFIG_DRM_KMS_HELPER)" != "y"  ||
      "$(get_kernel_config CONFIG_DRM_I915_KMS)" != "y" ]]
then
  message "${SPELL_COLOR}$SPELL${DEFAULT_COLOR}${PROBLEM_COLOR} requires" \
          "CONFIG_DRM_KMS_HELPER and CONFIG_DRM_I915_KMS built-in${DEFAULT_COLOR}" &&
  return 1
fi &&

default_pre_build
