           SPELL=telepathy-sofiasip
         VERSION=0.6.3
          SOURCE=$SPELL-$VERSION.tar.gz
         SOURCE2=$SOURCE.asc
        WEB_SITE=http://telepathy.freedesktop.org
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=$WEB_SITE/releases/$SPELL/$SOURCE
  SOURCE2_URL[0]=${SOURCE_URL[0]}.asc
      SOURCE_GPG=telepathy.gpg:$SOURCE.asc:UPSTREAM_KEY
  SOURCE2_IGNORE=signature
      LICENSE[0]=LGPL
         ENTERED=20070424
        KEYWORDS=""
           SHORT=" SIP-protocol connection manager for Telepathy"
cat << EOF
telepathy-sofiasip is a SIP-protocol connection manager (protocol plugin) for the Telepathy (http://telepathy.freedesktop.org) framework based on SofiaSIP-stack
