default_install                                                               &&
install  -m 644  zaptel.sysconfig         $INSTALL_ROOT/etc/sysconfig/zaptel  &&

local EXE &&
local BINS="torisatool makefw ztmonitor ztspeed zttest fxotune" &&
for EXE in $BINS; do
  install -m 544 $EXE $INSTALL_ROOT/sbin
done
