if  [ $RUN_YBIN = "y" ]; then
    if grep -q "boot=unconfigured" "/etc/yaboot.conf"; then
    	message "\n${PROBLEM_COLOR}You should configure your /etc/yaboot.conf,"\
	"particularly your bootstrap partition,\nthen run ybin -v as root."    \
        "$DEFAULT_COLOR"
    else
        /usr/sbin/ybin
    fi &&
fi
