          SOURCE=$SPELL-$VERSION.tar.bz2
   SOURCE_URL[0]=http://littlesvr.ca/$SPELL/releases/$SOURCE
     SOURCE_HASH=sha512:25ef3a8e4a381100ea8c1aa913f7ec28dc8b7c2c0f8bfa1cdcda529d69a76b7d8baf35ca3dfa087b7210f3ad53f340259717c099c188386b511e641a358aac93
