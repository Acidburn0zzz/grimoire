          SOURCE=$SPELL-$VERSION.tar.bz2
   SOURCE_URL[0]=http://littlesvr.ca/$SPELL/releases/$SOURCE
     SOURCE_HASH=sha512:c69639cc6a90d9d93f3c862d05742465015030a8c087b0137db25c937f793729a5f38569d57f4cdc2a7bb672dcd4b8db5e986529d2853bf9c90eb0954e76bb43
