            SPELL=cdrbq
          VERSION=1.23
           SOURCE=${SPELL}-${VERSION}.tgz
 SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
    SOURCE_URL[0]=$SOURCEFORGE_URL/cdrtoaster/$SOURCE
         WEB_SITE=http://cdrtoaster.sourceforge.net
          ENTERED=20020901
   SOURCE_HASH=sha512:0343eaa5758708ce0f91e3a91cc3d990f369a127dab1bb1e9672242df192822d9ec847cf4953ea17898e56fbe3b973584e345f4068a25c7921e063caeba0fcfd
