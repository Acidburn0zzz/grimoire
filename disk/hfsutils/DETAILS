           SPELL=hfsutils
         VERSION=3.2.6
          SOURCE=$SPELL-$VERSION.tar.gz
         SOURCE2=$SOURCE.sign
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=ftp://ftp.mars.org/pub/hfs/$SOURCE
  SOURCE2_URL[0]=${SOURCE_URL[0]}.sign
      SOURCE_GPG=F00BC095.gpg:$SOURCE2:UPSTREAM_KEY
