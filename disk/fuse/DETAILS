         VERSION=2.6.3
          SOURCE=$SPELL-$VERSION.tar.gz
   SOURCE_URL[0]=$SOURCEFORGE_URL/$SPELL/$SOURCE
     SOURCE_HASH=sha512:19907f5068871ed4207472faaa3bb8030a93ea574c6721cfc1a40d8b6d3365b84927c212ac047f1e46eb1b2969edc73b7225d984a79d0ad106a717b1fbe648b7
