          SOURCE=$SPELL-$VERSION.tar.gz
   SOURCE_URL[0]=$SOURCEFORGE_URL/$SPELL/$SOURCE
     SOURCE_HASH=sha512:d9eae22e59fef091dc45e9c97bca9a5ab77e4f0f7dc50cd53b59f3e7dc0b5849e198a90677c53b1c2430f49a69dd26385afdbdd54191a62dc766cece92d4f472
