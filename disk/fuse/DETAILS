         VERSION=2.6.4
          SOURCE=$SPELL-$VERSION.tar.gz
   SOURCE_URL[0]=$SOURCEFORGE_URL/$SPELL/$SOURCE
     SOURCE_HASH=sha512:e75167086053aaf9c3f61d511a4cd62a40138261b157323c2570d80030b476f4630b9547f54c54b4a3b51cd0fe1914cb73ce0027514e912a650fbfd30e3bc851
