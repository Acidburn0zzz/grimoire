           SPELL=fuse
if [ "$CVS" != "y" ]; then
         VERSION=2.8.1
     SOURCE_HASH=sha512:c2a7ae492a60119d330663bd4f11195360d4bcd3f6c4f232206c22962259c33ab798d0fa1327d7e4cb7d4f7aa57229182550054ba5972779fcc57424d1e8585f
          SOURCE=$SPELL-$VERSION.tar.gz
   SOURCE_URL[0]=$SOURCEFORGE_URL/$SPELL/$SOURCE
