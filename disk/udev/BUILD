OPTS="--prefix=$INSTALL_ROOT/usr \
      --sysconfdir=$INSTALL_ROOT/etc \
      --sbindir=$INSTALL_ROOT/sbin \
      --libdir=$INSTALL_ROOT/usr/lib \
      --with-rootlibdir=$INSTALL_ROOT/lib \
      --libexecdir=$INSTALL_ROOT/lib/udev \
      $UDEV_DEBUG \
      $UDEV_EXTRAS \
      $OPTS" &&

default_build
