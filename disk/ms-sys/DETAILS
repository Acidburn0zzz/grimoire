   SOURCE_URL[0]=http://downloads.sourceforge.net/sourceforge/${SPELL}/${SOURCE}
     SOURCE_HASH=sha512:10a17547cd4468eadca37ef03050a13686a7c6f9f6e41756532a60c74b5c7270636bacc0f9c15cf02d864d94c27386d81d14fdafcc9f90448c0852c0ad8db979
