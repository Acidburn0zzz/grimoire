           SPELL=konqueror4-plugins
persistent_read kdelibs4 KDE_VCS_AUTOUPDATE KDE_VCS_AUTOUPDATE
if [[ $KDE_VCS_AUTOUPDATE == y ]]; then
         VERSION=$(date +%Y%m%d)
else
         VERSION=svn
fi
   SOURCE_IGNORE=volatile
          SOURCE=$SPELL-svn.tar.bz2
   SOURCE_URL[0]=svn://anonsvn.kde.org/home/kde/trunk/extragear/base/konq-plugins:$SPELL
SOURCE_DIRECTORY="$BUILD_DIRECTORY/$SPELL-svn"
  FORCE_DOWNLOAD=1
        WEB_SITE=http://www.kde.org
         ENTERED=20090217
      LICENSE[0]=GPL
        KEYWORDS="kde4 konqueror4"
           SHORT="Plugins for Konqueror 4"
cat << EOF
This KDE module contains plugins that interact with Konqueror 4.
EOF
