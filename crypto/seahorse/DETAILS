           SPELL=seahorse

if [ "$SEAHORSE_DEVEL" == "y" ];then
         VERSION=0.9.3
     SOURCE_HASH=sha512:1b5e614b579a7c9afacd7566ff5c3f034391807aa6dc827b4b02f5923ec4f910e24ad709b5487e0e568d4bcc641b53a9cb3367550757947654b43b7ef179c23c
