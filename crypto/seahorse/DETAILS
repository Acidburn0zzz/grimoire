           SPELL=seahorse

if [ "$SEAHORSE_DEVEL" == "y" ];then
         VERSION=2.21.4
          SOURCE=$SPELL-$VERSION.tar.bz2
     SOURCE_HASH=sha512:9ee470817d8d403fc1e51722881b6dcc401420224b51e2f9e2f33a583e177629a017f3729ab3467374edafde5eeb9ca8036c2ae32b680f50ef7a2cc833a048d0
