           SPELL=seahorse

if [ "$SEAHORSE_DEVEL" == "y" ];then
         VERSION=2.22.0
          SOURCE=$SPELL-$VERSION.tar.bz2
     SOURCE_HASH=sha512:d4f56c805ee7c94222d355b540fbf20dbd23abba235c396d488da06ce51b2bd87ee5318a47c321ba315d3dd258870cf99dd8af46fc4aa665788d91dd6242b826
else
