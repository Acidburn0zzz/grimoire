           SPELL=seahorse

if [ "$SEAHORSE_DEVEL" == "y" ];then
         VERSION=0.9.2.1
     SOURCE_HASH=sha512:76467e9cbe4345d3a0677819450a5b5eb80c25b09dffc55a12b7c5ffef9f09d0584843c14d54b7281b032986b4f3239ef9f8781429bd28efc1430a0843c616b5
