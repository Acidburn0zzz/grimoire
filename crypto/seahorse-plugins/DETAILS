          SOURCE=$SPELL-$VERSION.tar.bz2
     SOURCE_HASH=sha512:8a8e2bde114a0596785d00cfd41c122582166bf2d5758c948f8b3ee27b45400389f9095f4e5036eec216d7ac14e87c5d18ad50975f6d81f847e51dbaf2392549
fi

          BRANCH=`echo $VERSION|cut -d . -f 1,2`
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=$GNOME_URL/sources/$SPELL/$BRANCH/$SOURCE
   SOURCE_URL[1]=http://ftp.gnome.org/pub/GNOME/sources/$SPELL/$BRANCH/$SOURCE
   SOURCE_URL[2]=$SOURCEFORGE_URL/$SPELL/$SOURCE
      LICENSE[0]=GPL
        WEB_SITE=http://www.gnome.org/projects/seahorse
         ENTERED=20080910
