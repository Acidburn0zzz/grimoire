           SPELL=seahorse-plugins

if [ "$SEAHORSE_PLUGINS_DEVEL" == "y" ];then
         VERSION=2.26.2
          SOURCE=$SPELL-$VERSION.tar.bz2
     SOURCE_HASH=sha512:28e79a5727424178d169d19e86923f4a0e13b567485ba741821ffcccc3c38bd74415921e21b0c54c099617fac6c809cd96e02fc1b09d50100c238197356897b8
else
