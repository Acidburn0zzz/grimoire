case $THIS_SUB_DEPENDS in
   3.12.x) echo "3.12.x release needed, forcing it."  &&
             NSS_BRANCH="3.12"                          ;;
   3.13.x) echo "3.13.x release needed, forcing it."  &&
             NSS_BRANCH="3.13"                          ;;
   PEM) echo "nss with pem needed, forcing it." &&
             NSS_PEM="y"                                ;;
   *)        echo "unknown sub-depends!"; return 1         ;;
esac
