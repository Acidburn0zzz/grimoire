default_pre_build  &&

cd $SOURCE_DIRECTORY &&

patch -p1 < $SCRIPT_DIRECTORY/nss-3.12-config-1.patch &&

if [[ "$NSS_PEM" == "y" ]]; then
tar jxvf $SCRIPT_DIRECTORY/nss-pem-20080124.tar.bz2 &&
patch -p0 < $SCRIPT_DIRECTORY/nss-enable-pem.patch &&
patch -p0 < $SCRIPT_DIRECTORY/nss-pem-bug429175.patch
fi
