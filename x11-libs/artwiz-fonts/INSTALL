FONT_DIR=${INSTALL_ROOT}/usr/share/fonts/misc
mkdir  -p  ${FONT_DIR}                                       &&
install -m 644 -o root -g root ${SOURCE_DIRECTORY}/*.{pcf,alias} \
	${FONT_DIR}
if list_find "$ART_ENC" "de"; then
	install -m 644 -o root -g root ${SOURCE2_DIRECTORY}/*.{pcf,alias} \
		${FONT_DIR}
fi                                                           &&
if list_find "$ART_ENC" "se"; then
	install -m 644 -o root -g root ${SOURCE3_DIRECTORY}/*.{pcf,alias} \
		${FONT_DIR}
fi
