           SPELL=freetype2
         VERSION=2.3.6
  SECURITY_PATCH=2
          SOURCE=freetype-$VERSION.tar.bz2
         SOURCE2=$SOURCE.sig
SOURCE_DIRECTORY=${BUILD_DIRECTORY}/freetype-${VERSION}
   SOURCE_URL[0]=http://download.savannah.gnu.org/releases/freetype/${SOURCE}   
   SOURCE_URL[1]=ftp://ftp.freetype.org/freetype/freetype2/${SOURCE}
  SOURCE2_URL[0]=$SOURCE_URL.sig
  SOURCE2_IGNORE=signature
      SOURCE_GPG="freetype.gpg:$SOURCE2:UPSTREAM_KEY"
        WEB_SITE=http://www.freetype.org
         ENTERED=20010922
      LICENSE[0]=GPL
        KEYWORDS="fonts graphics libs"
           SHORT="Free, quality, portable font engine."
cat << EOF
A Free, Quality, and Portable Font Engine
EOF
