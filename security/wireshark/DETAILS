   SOURCE_URL[1]=http://www.wireshark.org/download/src/$SOURCE
     SOURCE_HASH=sha512:9c0f1e3d6ebb31a235cc67139de0598509fdfbcaabef02d8383cd2410376c1b14f62ef1532aa29fef2e8dd5cc0a3655f57cc9a9fbb548936382ae51f2907339f
