           SPELL=gstm
         VERSION=1.2
          SOURCE=$SPELL-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=$SOURCEFORGE_URL/$SPELL/$SOURCE
     SOURCE_HASH=sha512:de04d75d64d111bce95bd51b52d738fd62d45e639c3534b877fda86447c9956d0baa40e80d56703270c6eb75377897a367b34b8d4fae35ce9f85f66e0ce24a6c
