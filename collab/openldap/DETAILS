          SOURCE=$SPELL-$VERSION.tgz
  SECURITY_PATCH=2
      SOURCE_URI=$SPELL-release/$SOURCE
   SOURCE_URL[0]=ftp://ftp.openldap.org/pub/OpenLDAP/$SOURCE_URI
