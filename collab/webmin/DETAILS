   SOURCE_URL[0]=$SOURCEFORGE_URL/webadmin/$SOURCE
     SOURCE_HASH=sha512:b23f458e1baaffa6e7496f8d18b0e031780ff0736abc371e3ff18cbd357ac61adabb709cb50d541494d367c9ff093e93e2bc3dc70d9d5a4003f8d89da64351f6
        WEB_SITE=http://www.webmin.com/
