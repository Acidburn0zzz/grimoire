if [[ -f "${INSTALL_ROOT}/etc/httpd/httpd.conf" ]]; then
    cp -v "${INSTALL_ROOT}/etc/httpd/httpd.conf" "${INSTALL_ROOT}/etc/httpd/httpd.conf.sorcerybackup"
fi &&

if [[ -f "${INSTALL_ROOT}/etc/httpsd/httpd.conf" ]]; then
    cp -v "${INSTALL_ROOT}/etc/httpsd/httpd.conf" "${INSTALL_ROOT}/etc/httpsd/httpd.conf.sorcerybackup"
fi
