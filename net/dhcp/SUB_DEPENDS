persistent_add DHCP_DBUS1
case "$THIS_SUB_DEPENDS" in
       DBUS) message "Requested sub_depends"
             DHCP_DBUS1=y &&
             depends dbus  &&
             depends dbus-glib
             ;;
*) message "${PROBLEM_COLOR}bogus sub_depends: $SPELL $THIS_SUB_DEPENDS${DEFAULT_COLOR}" ;
   return 1;;
esac
