      SOURCE_URL=$SOURCEFORGE_URL/$SPELL/$SOURCE
     SOURCE_HASH=sha512:6dc6ec515d6b58a7e365fdbe1084511ef4401022ca14fad30741bccbc79e3fdd5651c79662bfc63ff5f2192d3366924f041032380132af16359358c2cea72e0c
