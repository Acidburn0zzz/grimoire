      SOURCE_URL=$SOURCEFORGE_URL/$SPELL/$SOURCE
     SOURCE_HASH=sha512:eb831b193457f6c4f4cfa0997f3f62b3ffb1e56f3178db28c34b47f8cfab7d2927931bfe17936d783e85cef8960a045ac19107e73bc84af4e1482f4cdc254768
