   SOURCE_URL[0]=http://samplicator.googlecode.com/files/$SOURCE
     SOURCE_HASH=sha512:a4023344ec911d3dc6992ddce139e6f1c86a67bd9948a3f92a995dc26aeda1753becea4b0f5d9ad45a06d628029bffc0a77c28fbd277da9faf42100c01d55f31
