cd $SOURCE_DIRECTORY &&
cp snmptt /usr/sbin                                 &&
cp snmptthandler /usr/sbin                          &&
cp snmpttconvert /usr/bin                           &&
cp snmpttconvertmib /usr/bin                        &&
cp snmptthandler /usr/bin                           &&

mkdir -p /etc/snmp                                  &&
install_config_file snmptt.ini /etc/snmp/snmptt.ini

