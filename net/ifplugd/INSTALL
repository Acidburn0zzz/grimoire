default_install  &&

# Install this separately, otherwise the existing one is overwritten
install_config_file  $SOURCE_DIRECTORY/conf/ifplugd.conf    \
                               /etc/ifplugd/ifplugd.conf    &&
install_config_file  $SOURCE_DIRECTORY/conf/ifplugd.action  \
                               /etc/ifplugd/ifplugd.action
