cd  $SOURCE_DIRECTORY  &&

patch -p1 < "$SPELL_DIRECTORY/fsctl.patch" &&
patch -p0 < "$SPELL_DIRECTORY/logs.patch" &&

#sedit  's!libsmbclient.*\$(LIBDIR)!&/..!'  source/Makefile.in  &&

if [[ "$SAMBA_VSCAN" == "y" ]] ; then
        cd $SOURCE_DIRECTORY/examples/VFS/ &&
        unpack_file 2 &&
        cd $SOURCE_DIRECTORY
fi
