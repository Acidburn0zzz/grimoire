default_pre_build      &&
cd  $SOURCE_DIRECTORY  &&

if is_depends_enabled $SPELL cups; then
  zcat "$SPELL_DIRECTORY/cups16.patch" | patch -p1
fi &&

#sedit  's!libsmbclient.*\$(LIBDIR)!&/..!'  source/Makefile.in  &&

if [[ "$SAMBA_VSCAN" == "y" ]] ; then
        cd $SOURCE_DIRECTORY/examples/VFS/ &&
        unpack_file 2 &&
        cd $SOURCE_DIRECTORY
fi
