           SPELL=dante
         VERSION=1.2.0
          SOURCE=$SPELL-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=ftp://ftp.inet.no/pub/socks/$SOURCE
        WEB_SITE=http://www.inet.no/dante/
         ENTERED=20060609
