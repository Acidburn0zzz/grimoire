           SPELL=libnfnetlink
if  [[  "$LIBNFNETLINK_SVN_AUTOUPDATE"  ==  "y"  ]];  then
         VERSION=$(date +%Y%m%d)
else
         VERSION=git
fi
          SOURCE=$SPELL-git.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_IGNORE=volatile
   SOURCE_URL[0]=git://git.netfilter.org/libnfnetlink.git:$SPELL-git
  FORCE_DOWNLOAD=1
        WEB_SITE=http://www.netfilter.org
         ENTERED=20060413
      LICENSE[0]=GPL
           SHORT="netfilter libnfnetlink"
cat << EOF
libnfnetlink is the low-level library for netfilter related kernel/userspace communication.
EOF
