           SPELL=libnfnetlink
if  [[  "$LIBNFNETLINK_SVN_AUTOUPDATE"  ==  "y"  ]];  then
         VERSION=$(date +%Y%m%d)
else
         VERSION=svn
fi
          SOURCE=$SPELL-svn.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_IGNORE=volatile
         NFT_SVN='https://svn.netfilter.org/netfilter/trunk/libnfnetlink/'
         WEBSITE=http://www.netfilter.org
         ENTERED=20060413
      LICENSE[0]=GPL
           SHORT="netfilter libnfnetlink"
cat << EOF
libnfnetlink is the low-level library for netfilter related kernel/userspace communication.
EOF
