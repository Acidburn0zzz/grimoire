           SPELL=dhcpcd
if [[ $DHCPCD_DEVEL == y ]]; then
         VERSION=5.0.1
     SOURCE_HASH=sha512:e67cab10cef5584844e30c0a99fc8ac11bca0a3e26f8a61e05bbbf353f4bb2174e0c0c87f208f21e33f68ee440ea4990857e4d98b58cab8f5030834bf6cc4a46
else
         VERSION=5.0.1
     SOURCE_HASH=sha512:e67cab10cef5584844e30c0a99fc8ac11bca0a3e26f8a61e05bbbf353f4bb2174e0c0c87f208f21e33f68ee440ea4990857e4d98b58cab8f5030834bf6cc4a46
