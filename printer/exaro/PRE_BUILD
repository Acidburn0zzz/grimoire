if [[ $EX_VER == y ]];then
  cd $BUILD_DIRECTORY &&
  unlzma -c -d $SOURCE_CACHE/$SOURCE | tar x
else
default_pre_build
fi
cd $SOURCE_DIRECTORY &&
if [[ $EX_VER == n ]];then
sed -i 's/libs//' exaro.pro &&
