default_pre_build                      &&
cd ${SOURCE_DIRECTORY}                 &&
verify_file 2                          &&
verify_file 3                          &&
tar vxf $SOURCE_CACHE/$SOURCE2 --strip-components 1  &&
tar vxf $SOURCE_CACHE/$SOURCE3 --strip-components 1  &&

message "${MESSAGE_COLOR}patches 001-100${DEFAULT_COLOR}"
unpack_file 4 &&
gunzip -c $SOURCE_CACHE/$SOURCE4 | patch -p0 &&

message "${MESSAGE_COLOR}patches 101-200${DEFAULT_COLOR}"
unpack_file 5 &&
gunzip -c $SOURCE_CACHE/$SOURCE5 | patch -p0 &&

message "${MESSAGE_COLOR}patches 201-300${DEFAULT_COLOR}"
unpack_file 6 &&
gunzip -c $SOURCE_CACHE/$SOURCE6 | patch -p0 &&

message "${MESSAGE_COLOR}patches 301-400${DEFAULT_COLOR}"
unpack_file 7 &&
gunzip -c $SOURCE_CACHE/$SOURCE7 | patch -p0 &&

for patch in $(seq 401 411); do
  message "${MESSAGE_COLOR}patch $patch${DEFAULT_COLOR}" &&
  zcat "$SPELL_DIRECTORY/patches/$VERSION.$patch.gz" | patch -p0
done
