default_pre_build     &&
cd $SOURCE_DIRECTORY  &&

# Fix autoconf detection for 2.6x, fix error message wording.
sedit  's/\*2.5\*/*2.[56]*/g'                 admin/cvs.sh  &&
sedit  's/2.52, 2.53 or 2.54/2.5x or 2.6x/g'  admin/cvs.sh  &&
sedit  's/2.52 or 2.53/2.5x or 2.6x/g'        admin/cvs.sh  &&

make -f Makefile.cvs
