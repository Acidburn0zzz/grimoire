source $SECTION_DIRECTORY/TEX_DEPENDS &&
if [[ "$TETEX_TEXMF_CURRENT" == "y" ]]; then
	depends latex-beamer &&
	depends pgf          &&
	depends xcolor       &&
	depends koma-script  && 
	depends ifxetex
else
	conflicts latex-beamer &&
	conflicts pgf          &&
	conflicts xcolor       &&
	conflicts koma-script  &&
	conflicts ifxetex
fi
