. $GRIMOIRE/libcompat  &&
depends  gcc           &&

if [[ $GCJ_AWT == y ]]; then
  depends  gtk+2 '--enable-java-awt=gtk'  &&
  depends  libart_lgpl                    &&
  depends  libxtst
fi  &&

if [[ $VERSION != $(installed_version gcc) ]]; then
  force_depends gcc
fi &&

depends  gmp             &&
depends  mpfr            &&
depends  smgl-archspecs  &&
depends  unzip           &&
depends  zip
