           SPELL=omins
         VERSION=0.2.0
          SOURCE=$SPELL-$VERSION.tar.gz
         SOURCE2=$SOURCE.sig
   SOURCE_URL[0]=http://download.savannah.gnu.org/releases/om-synth/$SOURCE
  SOURCE2_URL[0]=${SOURCE_URL[0]}.sig
