default_pre_build        &&
cd  "$SOURCE_DIRECTORY"  &&

sed -i  "s:/usr/local/lib/ladspa/:\"$INSTALL_ROOT/usr/lib/ladspa/\":"  \
        Makefile                                                       &&

sed -i  "s:/usr/local/share/ladspa/rdf/:\"$INSTALL_ROOT/usr/share/ladspa/rdf/\":" \
        Makefile
