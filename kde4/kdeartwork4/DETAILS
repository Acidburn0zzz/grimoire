  if [[ "${KDE_VER}" == "unstable" ]];then
         VERSION=4.2.98
     SOURCE_HASH=sha512:fa6174e39fdcb09d59231e27fd4a2a7c5d0f4c26dac76f1bb440132ab88686d77990a13f484562ef99b4804477b97b48fb93ae81ed7b6edc736832fae2e52931
