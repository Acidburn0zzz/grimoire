default_pre_build    &&
cd $SOURCE_DIRECTORY &&
if [[ $KDE_VER == unstable ]]; then
# for version 4.2.85
# sane problem applies to unstable version
patch -p1 < $SPELL_DIRECTORY/sane.diff  &&
# the FindKdcraw.cmake from kdelibs4-4.2.85 is broken
cp $SPELL_DIRECTORY/FindKdcraw.cmake cmake/modules/
fi
