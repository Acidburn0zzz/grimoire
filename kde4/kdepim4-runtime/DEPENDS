source  $SECTION_DIRECTORY/KDE_DEPENDS &&
depends kdepimlibs4  &&
depends akonadi      &&
depends boost        &&
if [[ "${KDE_VER}" == "stable" ]];then
  depends kdelibs4-experimental
fi &&
optional_depends soprano       '' '' 'Nepomuk support' &&
if is_depends_enabled $SPELL soprano;then
  sub_depends kdelibs4 SOPRANO
fi

