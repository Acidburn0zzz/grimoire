create_group  kde4    &&
default_pre_build     &&
cd $SOURCE_DIRECTORY  &&
if [[ "${KDE_VER}" == "unstable" ]];then
# for new libxcb
  if [[ `installed_version libxcb` != "1.1" ]];then
    sed -i 's|X11/extensions/XTest|xcb/xtest|' plasma/netbook/applets/currentappcontrol/currentappcontrol.cpp
  fi
fi
