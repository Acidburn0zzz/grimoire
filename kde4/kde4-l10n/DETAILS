# the remaining details of this spell are provided by CONFIGURE
           SPELL=kde4-l10n
          SPELLX=kde-l10n
persistent_read kdelibs4 KDE_VER KDE_VER
if [[ $KDE_VER != stable ]]; then
         VERSION=4.1.64
else
         VERSION=4.1.2
fi
            SITE=$KDE_URL/${KDE_VER:-stable}/${VERSION}/src/$SPELLX
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELLX
source $SPELL_CONFIG
        WEB_SITE=http://www.kde.org
         ENTERED=20070909
      LICENSE[0]=GPL
         ARCHIVE=off
        KEYWORDS="kde"
           SHORT="Translated KDE messages."
cat << EOF
kde4-l10n contains translated KDE messages.
EOF
