   SOURCE_URL[1]=ftp://ftp.gnu.org/pub/gnu/autoconf/$SOURCE
      SOURCE_HASH=sha512:602584f4c77b7a554aaa068eda5409b68eb0b3229e9c224bffb91c83c4314d25de15bd560a323626ff78f6df339c79e1ef8938c54b78ecadf4dc75c5241290ad
