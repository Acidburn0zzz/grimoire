           SPELL=shiboken
        WEB_SITE=http://www.pyside.org/
if [[ $PYSIDE == git ]];then
         VERSION=git
          SOURCE=$SPELL-$VERSION.tar.bz2
   SOURCE_URL[0]=git://gitorious.org/pyside/$SPELL.git:$SPELL-git
  FORCE_DOWNLOAD=on
   SOURCE_IGNORE=volatile
else
         VERSION=0.4.0
     SOURCE_HASH=sha512:d1efcc86648df39980d207b9ac37c499f5cbf58477d626e6f613e644e202737dead14175f86fd6d493f8a08993bdbca90964ffb030394ff1ceeeeeab48eeaaeb
          SOURCE=$SPELL-$VERSION.tar.bz2
   SOURCE_URL[0]=$WEB_SITE/files/$SOURCE
fi
