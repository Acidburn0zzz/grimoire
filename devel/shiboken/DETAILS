           SPELL=shiboken
        WEB_SITE=http://www.pyside.org/
if [[ $PYSIDE == git ]];then
         VERSION=git
          SOURCE=$SPELL-$VERSION.tar.bz2
   SOURCE_URL[0]=git://gitorious.org/pyside/$SPELL.git:$SPELL-git
  FORCE_DOWNLOAD=on
   SOURCE_IGNORE=volatile
else
         VERSION=0.5.0
     SOURCE_HASH=sha512:6a58d8a9d5232c5ca291ec421f44d494d7c073d50c3066f6eeef2daab0d0d468bcad78d470670170e6e1bbffb893e669988df49d99d79a37fe4344e8650c8105
          SOURCE=$SPELL-$VERSION.tar.bz2
