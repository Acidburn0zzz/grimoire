           SPELL=shiboken
        WEB_SITE=http://www.pyside.org
if [[ $PYSIDE == git ]];then
         VERSION=git
          SOURCE=$SPELL-$VERSION.tar.bz2
   SOURCE_URL[0]=git://gitorious.org/pyside/$SPELL.git:$SPELL-git
  FORCE_DOWNLOAD=on
   SOURCE_IGNORE=volatile
else
         VERSION=1.0.0~rc1
     SOURCE_HASH=sha512:8c9dcbc20ca137348ba281db41ee9296cde2efdb56dbb8db260fa7e7e99868088f031161a3c71858f82e699fb4bd81629086fc48c54bae6fd8d2abc12e10d8e9
          SOURCE=$SPELL-$VERSION.tar.bz2
   SOURCE_URL[0]=$WEB_SITE/files/$SOURCE
fi
