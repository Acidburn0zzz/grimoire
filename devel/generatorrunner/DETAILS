           SPELL=generatorrunner
        WEB_SITE=http://www.pyside.org/
if [[ $PYSIDE == git ]];then
         VERSION=git
          SOURCE=$SPELL-$VERSION.tar.bz2
   SOURCE_URL[0]=git://gitorious.org/pyside/$SPELL.git:$SPELL-git
  FORCE_DOWNLOAD=on
   SOURCE_IGNORE=volatile
else
         VERSION=0.3.2
     SOURCE_HASH=sha512:6c06bee86788daadac245630d27bdc23bf8e9ff55abcdf82a4200c42e988df59ad0c8740aa031c63a2ba1986c50421830a22c9664c857fa21aabfde761325cf3
          SOURCE=$SPELL-$VERSION.tar.bz2
