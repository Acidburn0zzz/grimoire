           SPELL=generatorrunner
        WEB_SITE=http://www.pyside.org/
if [[ $PYSIDE == git ]];then
         VERSION=git
          SOURCE=$SPELL-$VERSION.tar.bz2
   SOURCE_URL[0]=git://gitorious.org/pyside/$SPELL.git:$SPELL-git
  FORCE_DOWNLOAD=on
   SOURCE_IGNORE=volatile
else
         VERSION=0.6.7
     SOURCE_HASH=sha512:c1ac04032af18585f995c028c1b89a50697d742522180ef897dab22a035322a9d8cbbf98851f8c8cb4dbde6c3d1b5e51ce4199bc25ef2481d3c6a4e7c2f3bd5d
