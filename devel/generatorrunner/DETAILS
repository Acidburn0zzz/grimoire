           SPELL=generatorrunner
        WEB_SITE=http://www.pyside.org/
if [[ $PYSIDE == git ]];then
         VERSION=git
          SOURCE=$SPELL-$VERSION.tar.bz2
   SOURCE_URL[0]=git://gitorious.org/pyside/$SPELL.git:$SPELL-git
  FORCE_DOWNLOAD=on
   SOURCE_IGNORE=volatile
else
         VERSION=0.6.11
     SOURCE_HASH=sha512:648856ec6bf3a8336974046d317b808564aaeff1d90a53e7f46b9b66a253750c00554cf087ce84a71c178cec4d1e90beb399366e7edaba451c4987c1e5d170e6
