           SPELL=generatorrunner
        WEB_SITE=http://www.pyside.org/
if [[ $PYSIDE == git ]];then
         VERSION=git
          SOURCE=$SPELL-$VERSION.tar.bz2
   SOURCE_URL[0]=git://gitorious.org/pyside/$SPELL.git:$SPELL-git
  FORCE_DOWNLOAD=on
   SOURCE_IGNORE=volatile
else
         VERSION=0.4.0
     SOURCE_HASH=sha512:c93f52e2a9097b5b9714eac47312da4346b751241ad5c043cf978aa98fab411e03703c9b94c68f3e37d002128cc61e44d2a3e216aba1998274188eb5c520bc88
