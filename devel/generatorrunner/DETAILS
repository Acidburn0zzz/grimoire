           SPELL=generatorrunner
        WEB_SITE=http://www.pyside.org/
if [[ $PYSIDE == git ]];then
         VERSION=git
          SOURCE=$SPELL-$VERSION.tar.bz2
   SOURCE_URL[0]=git://gitorious.org/pyside/$SPELL.git:$SPELL-git
  FORCE_DOWNLOAD=on
   SOURCE_IGNORE=volatile
else
         VERSION=0.3.3
     SOURCE_HASH=sha512:f7c7d29a3ec53859bce62aea8865423080a6a9b726dc5f0f2f5b29bc94fc733c887bf9baa33ed15da33a5cbba9c3c687f3299251b50f94e845aabd403267c611
