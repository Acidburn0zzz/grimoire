else
         VERSION=0.6.15
     SOURCE_HASH=sha512:83d6d1e7faa467e99e04997fd8eb3a4dbf51dccdc456236a50f63361717c862b1530cd218631b919947df90877340593741f455c2c174b201b44cf4987624eb4
