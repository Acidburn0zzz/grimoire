           SPELL=generatorrunner
        WEB_SITE=http://www.pyside.org/
if [[ $PYSIDE == git ]];then
         VERSION=git
          SOURCE=$SPELL-$VERSION.tar.bz2
   SOURCE_URL[0]=git://gitorious.org/pyside/$SPELL.git:$SPELL-git
  FORCE_DOWNLOAD=on
   SOURCE_IGNORE=volatile
else
         VERSION=0.6.8
     SOURCE_HASH=sha512:db95470399bea87bfd7ceb7649d85c01a969bb4f6cbdbab728b32289e6c2bbd6eb81894b703a4009c35bcfe442e6731cfb8e053afbd23d85bb5a796443791680
          SOURCE=$SPELL-$VERSION.tar.bz2
