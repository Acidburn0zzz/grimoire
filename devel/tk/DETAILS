            SPELL=tk
if [ "$DEVEL" = "y" ]; then
          VERSION=8.6a3
           SOURCE=tcl$VERSION-src.tar.gz
          SOURCE2=tk$VERSION-src.tar.gz
       SOURCE_GPG="gurus.gpg:${SOURCE}.sig:WORKS_FOR_ME"
      SOURCE2_GPG="gurus.gpg:${SOURCE2}.sig:WORKS_FOR_ME"
