if ! spell_ok fpc ; then
case "${SMGL_COMPAT_ARCHS[1]}" in
ia32)     B_ARCH=i386-linux ARCH=386 ;;
*)        B_ARCH=${SMGL_COMPAT_ARCHS[1]}-linux ARCH=x64 ;;
esac
         SOURCE2=$SPELL-$VERSION.$B_ARCH.tar
