           SPELL=pyside
        WEB_SITE=http://www.pyside.org/
if [[ $PYSIDE == git ]];then
         VERSION=git
          SOURCE=$SPELL-$VERSION.tar.bz2
   SOURCE_URL[0]=git://gitorious.org/pyside/$SPELL.git:$SPELL-git
  FORCE_DOWNLOAD=on
   SOURCE_IGNORE=volatile
else
         VERSION=qt4.7+1.0.0~beta3
     SOURCE_HASH=sha512:ba0f20b42078d0fb9c9899e2b6b101898ec943e6cecd75d907e4c7c701e0a8fe3507891089869bf7fef7e02cc16029b36a5025291db17dbf739ac551e39d66ef
