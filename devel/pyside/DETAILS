           SPELL=pyside
        WEB_SITE=http://www.pyside.org/
if [[ $PYSIDE == git ]];then
         VERSION=git
          SOURCE=$SPELL-$VERSION.tar.bz2
   SOURCE_URL[0]=git://gitorious.org/pyside/$SPELL.git:$SPELL-git
  FORCE_DOWNLOAD=on
   SOURCE_IGNORE=volatile
else
         VERSION=qt4.7+1.0.1
     SOURCE_HASH=sha512:620953139d78d357d9d9b5b8d64fcf92aedf361d0d01e37bd5f83daf5807dda3dc5f68a9ba0cdd82b20ded7e2d3eea8c114129a85be1836242a67c8991f74754
