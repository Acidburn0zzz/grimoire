           SPELL=pyside
        WEB_SITE=http://www.pyside.org/
if [[ $PYSIDE == git ]];then
         VERSION=git
          SOURCE=$SPELL-$VERSION.tar.bz2
   SOURCE_URL[0]=git://gitorious.org/pyside/$SPELL.git:$SPELL-git
  FORCE_DOWNLOAD=on
   SOURCE_IGNORE=volatile
else
         VERSION=qt4.7+1.0.7
     SOURCE_HASH=sha512:4d39dbb2ed816ca47207973b05f88f67fb6a99d303eb8d83f3c3b8f09b8b93bf836c5efdcc968de06f48becf022d91425a0369c48d63b0d3e6adec447b4193d3
