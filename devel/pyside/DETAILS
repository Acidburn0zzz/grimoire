           SPELL=pyside
        WEB_SITE=http://www.pyside.org/
if [[ $PYSIDE == git ]];then
         VERSION=git
          SOURCE=$SPELL-$VERSION.tar.bz2
   SOURCE_URL[0]=git://gitorious.org/pyside/$SPELL.git:$SPELL-git
  FORCE_DOWNLOAD=on
   SOURCE_IGNORE=volatile
else
         VERSION=qt4.7+1.0.0~rc1
     SOURCE_HASH=sha512:50683681348b20716bafe5597e84d43a61b7e7fe80528d11254fbe06b53911d3a42d9ed3f9a62d0bec9bee1c3d553c669f5eb83b4f8b81341c6473fc8de9aa34
