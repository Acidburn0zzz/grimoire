           SPELL=pyside
        WEB_SITE=http://www.pyside.org/
if [[ $PYSIDE == git ]];then
         VERSION=git
          SOURCE=$SPELL-$VERSION.tar.bz2
   SOURCE_URL[0]=git://gitorious.org/pyside/$SPELL.git:$SPELL-git
  FORCE_DOWNLOAD=on
   SOURCE_IGNORE=volatile
else
         VERSION=0.1.4.5
     SOURCE_HASH=sha512:e7319805af239dd8c4b8fdb7b71c9ab9fe7ff5b06d464288696ea0997c13ef1cc3b8087c0e568699510158eedd5e203fb637176489f926f69b8a8c01fc55f6f0
