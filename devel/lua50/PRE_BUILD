default_pre_build       &&
cd "$SOURCE_DIRECTORY"  &&

sedit "s:^INSTALL_ROOT=.*:INSTALL_ROOT=$INSTALL_ROOT/usr:" \
      config            &&
sedit "s:^INSTALL_MAN=.*:INSTALL_ROOT=$INSTALL_ROOT/usr/share/man/man1:" \
      config            &&
sedit "s:^MYCFLAGS=.*:MYCFLAGS=${CFLAGS}:" \
      config

