else
         VERSION=0.1.2
     SOURCE_HASH=sha512:57033a2593c10177371a9bbf8888bf14a348d927ea944901792e3673efec1f1ac7305a99cb609c613628bd9c0ff80a9875cd1617e1b5bb1a2cf916fd745350e2
