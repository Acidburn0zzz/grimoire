           SPELL=pyside-tools
        WEB_SITE=http://www.pyside.org
if [[ $PYSIDE == git ]];then
         VERSION=git
          SOURCE=$SPELL-$VERSION.tar.bz2
   SOURCE_URL[0]=git://gitorious.org/pyside/$SPELL.git:$SPELL-git
  FORCE_DOWNLOAD=on
   SOURCE_IGNORE=volatile
else
         VERSION=0.2.6
     SOURCE_HASH=sha512:b2a1339488a39848686acc3391e55749b4946ac58ed03ef350559026931ffb90b05a4d45dd1a62f0b36b837be15bc074250d4e35200d63848b3095fb60ad24e6
          SOURCE=$SPELL-$VERSION.tar.bz2
