           SPELL=pyside-tools
        WEB_SITE=http://www.pyside.org
if [[ $PYSIDE == git ]];then
         VERSION=git
          SOURCE=$SPELL-$VERSION.tar.bz2
   SOURCE_URL[0]=git://gitorious.org/pyside/$SPELL.git:$SPELL-git
  FORCE_DOWNLOAD=on
   SOURCE_IGNORE=volatile
else
         VERSION=0.2.1
     SOURCE_HASH=sha512:1dbbfe0d63dea679e40f12fd0ec8666e2ac1ff8fc25ee6d78338c60b84f2099e4c9cecb1972aeff890d75bd7ce638ebe2117f105bb2bb1b12188e7b5d75c809d
