#default_install
cd $SOURCE_DIRECTORY/fst                    &&
cp lib/fstlib.h  $INSTALL_ROOT/usr/include  &&
#DEST=$INSTALL_ROOT/usr/lib/fst   &&
#mkdir -p $DEST                   &&
cp {bin,lib}/*.so $INSTALL_ROOT/usr/lib     &&
for FF in bin/fst*; do
  if [ -x $FF ]; then
#    ls -l $FF
    cp $FF $INSTALL_ROOT/usr/bin
  fi
done 
