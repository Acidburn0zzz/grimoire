                             "--strip-binaries" "" &&

config_query_option TCC_OPTS "Make libtcc.so instead of libtcc.a?" n \
                             "--disable-static" "" &&

config_query_option TCC_OPTS "Enable gprof?" n \
                             "--enable-gprof" ""
