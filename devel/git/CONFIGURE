config_query_list GIT_SHA_OPT \
                  "Which sha option would you like to use?" \
                  openssl \
                  mozilla &&

if spell_ok uri &&
   spell_ok term-readkey &&
   spell_ok lwp; then
  config_query GIT_SVN "Enable Subversion import support?" y
else
  config_query GIT_SVN "Enable Subversion import support?" n
fi
