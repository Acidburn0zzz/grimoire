    SOURCE3_HASH=sha512:dfdbe2ddeea88468767d6a0327848dce63f7baa0a6733ef383361636097b9e977bb09bcb2a0a5c1020fa118484a37a6b13615fdb605eb11369eb38e57c9fa556
     SOURCE3_URL=$KERNEL_URL/pub/software/scm/git/$SOURCE3
