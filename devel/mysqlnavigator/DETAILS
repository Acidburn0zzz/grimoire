   SOURCE_URL[0]=$SOURCEFORGE_URL/mysqlnavigator/$SOURCE
   SOURCE_URL[1]=http://sql.kldp.org/snapshots/$SOURCE
   SOURCE_URL[2]=http://ftp.kde.com/Database/MySQL_Navigator/$SOURCE
