           SPELL=e_module-emu
  if  [[  "$E_MODULE_EMU_CVS_AUTOUPDATE"  ==  "y"  ]];  then
         VERSION=$(date +%Y%m%d)
  else
         VERSION=cvs
  fi
          SOURCE=$SPELL-cvs.tar.bz2
