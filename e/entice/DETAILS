           SPELL=entice
if  [  "$ENTICE_CVS_AUTOUPDATE"  ==  "y"  ];  then
         VERSION=$(date +%Y%m%d)
else
         VERSION=cvs
fi
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-cvs
        WEB_SITE=http://www.enlightenment.org/pages/entice.html
         ENTERED=20040311
      LICENSE[0]=BSD
      PATCHLEVEL=1
