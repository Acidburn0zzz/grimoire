           SPELL=e_module-devian
if  [[  "$E_MODULE_DEVIAN_CVS"  ==  "y"  ]];  then
  if  [[  "$E_MODULE_DEVIAN_CVS_AUTOUPDATE"  ==  "y"  ]];  then
         VERSION=$(date +%Y%m%d)
  else
         VERSION=cvs
  fi
else
         VERSION=0.1.7
SOURCE_DIRECTORY=$BUILD_DIRECTORY/dEvian-$VERSION
fi
