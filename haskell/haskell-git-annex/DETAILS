          SOURCE="${XSPELL}-${VERSION}.tar.gz"
   SOURCE_URL[0]=http://hackage.haskell.org/packages/archive/${XSPELL}/${VERSION}/${SOURCE}
     SOURCE_HASH=sha512:29a39883693f2ebd374d3063bc13c326afc448de900a31bf6dc05bb28e9a8c146877138cade757d86603add53679e9718aa7f8405c56770c46120bd3891b3028
