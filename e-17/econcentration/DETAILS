           SPELL=econcentration
  if  [[  "$E_CONCENT_SVN_AUTOUPDATE"  ==  "y"  ]];  then
         VERSION=$(date +%Y%m%d)
  else
         VERSION=svn
  fi
          SOURCE=$SPELL-svn.tar.bz2
   SOURCE_URL[0]=svn_http://svn.enlightenment.org/svn/e/trunk/GAMES/$SPELL:$SPELL
   SOURCE_IGNORE=volatile
