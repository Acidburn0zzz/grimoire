           SPELL=e_module-systray
  if  [[  "$E_MODULE_SYSTRAY_AUTOUPDATE"  ==  "y"  ]];  then
         VERSION=$(date +%Y%m%d)
  else
         VERSION=svn
  fi
          SOURCE=$SPELL-svn.tar.bz2
   SOURCE_URL[0]=svn_http://svn.enlightenment.org/svn/e/trunk/E-MODULES-EXTRA/${SPELL/*-/}:$SPELL
   SOURCE_IGNORE=volatile
  FORCE_DOWNLOAD="on"
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-svn
      LICENSE[0]=BSD
        WEB_SITE=http://www.enlightenment.org/
         ENTERED=20090325
           SHORT='E17 System Tray'
cat << EOF
E17 System Tray hold applications icons like Skype, Pidgin, Kopete
and others. Implemented following freedesktop.org standard.
EOF
