           SPELL=e17-theme-b_and_w
if  [  "$B_AND_W_SCM_AUTOUPDATE"  ==  "y"  ];  then
         VERSION=$(date +%Y%m%d)
else
         VERSION=svn
fi
          SOURCE=$SPELL-svn.tar.bz2
