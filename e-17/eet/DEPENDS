depends  eina      &&
depends  JPEG      &&
depends  zlib      &&
depends  libgcrypt &&

if [[ $EET_BRANCH == scm ]]; then
  depends  subversion
fi  &&

if [ "$EET_CRYPTO" == "gnutls" ]; then
   depends gnutls "--enable-gnutls --disable-openssl --enable-cipher --enable-signature"
elif [ "$EET_CRYPTO" == "openssl" ]; then
   depends openssl "--enable-openssl --disable-gnutls --enable-cipher --enable-signature"
fi

