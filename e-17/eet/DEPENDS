depends  eina  &&
depends  JPEG  &&

if [[ $EET_BRANCH == scm ]]; then
  depends  subversion
fi  &&

optional_depends  openssl                                                   \
                  '--enable-openssl --enable-cypher --enable-signature'     \
                  '--disable-openssl --disable-cypher --disable-signature'  \
                  'for SSL signature support'
