           SPELL=e_utils
if  [  "$E_UTILS_CVS_AUTOUPDATE"  ==  "y"  ];  then
         VERSION=$(date +%Y%m%d)
else
         VERSION=svn
fi
          SOURCE=$SPELL-svn.tar.bz2
