          SOURCE=$SPELLX-$VERSION.tar.gz
   SOURCE_URL[0]=$PERL_CPAN_URL/CPAN/authors/id/T/TI/TIMB/$SOURCE
     SOURCE_HASH=sha512:8d541ada1fabb7e82dd4f47c43edf79cc70c783ef3582a9911b13239734876aee9f27095f2526e6923d07b7ac86df48ee081af1ca6000df0559a13c75b6f6ed3
