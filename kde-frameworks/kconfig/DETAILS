           SPELL=kconfig
         VERSION=5.14.0
       VX=${VERSION%.?}
     SOURCE_HASH=sha512:1ddd59811991e5ae4ec546032055b84ea1fcc99cad839fcdd1d2127c0fe28a46099c80379901f2be22cb08e68c971b330cffdf8b328cb596d785167371ba3fba
          SOURCE=$SPELL-$VERSION.tar.xz
      SOURCE_URL=http://download.kde.org/stable/frameworks/$VX/$SOURCE
