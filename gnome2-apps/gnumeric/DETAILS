          BRANCH=`echo $VERSION|cut -d . -f 1,2`
SOURCE_DIRECTORY="$BUILD_DIRECTORY/$SPELL-$VERSION"
     SOURCE_HASH=sha512:241a80d79d1d20f484b2e262b86c6dd12192842b5baf7f11c3402b63810f1e67080b6ec44f05a1de88ae3f0cd476cd9ffb2ecbebe2cdf9104bb45287c8e5b8b9
