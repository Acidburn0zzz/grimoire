config_query GNOME_SYSTEMTOOLS_NETWORK  "Enable network module?" y          &&
config_query GNOME_SYSTEMTOOLS_SERVICES "Enable services module?" y         &&
config_query GNOME_SYSTEMTOOLS_TIME     "Enable time module?" y             &&
config_query GNOME_SYSTEMTOOLS_USERS    "Enable users module?" y            &&
config_query GNOME_SYSTEMTOOLS_SHARES   "Enable shares module?" y           &&
config_query GNOME_SYSTEMTOOLS_NAUTILUS "Enable Nautilus shares module?" y
