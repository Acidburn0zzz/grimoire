          SOURCE=${SPELL}-${XORG_VER}-${VERSION}.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/${SPELL}-${XORG_VER}-${VERSION}
   SOURCE_URL[0]=http://xorg.freedesktop.org/releases/${XORG_VER}/src/proto/${SOURCE}
      SOURCE_GPG="gurus.gpg:${SOURCE}.sig"
