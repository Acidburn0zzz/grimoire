if spell_ok gstreamer; then
   config_query_list TRACKER_EXTRACTOR "Which video extractor to use?" \
                                       gstreamer xine                 
elif spell_ok xine-lib; then
   config_query_list TRACKER_EXTRACTOR "Which video extractor to use?" \
                                       xine gstreamer                 
else
   config_query_list TRACKER_EXTRACTOR "Which video extractor to use?" \
                                       gstreamer xine                 
fi  &&

config_query_list TRACKER_MONITOR "Which file monitoring to use?"      \
                                   inotify fam polling
