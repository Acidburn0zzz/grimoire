   SOURCE_URL[0]=$GNOME_URL/sources/gdk-pixbuf/$BRANCH/$SOURCE
   SOURCE_URL[1]=ftp://ftp.gnome.org/pub/GNOME/sources/gdk-pixbuf/$BRANCH/$SOURCE
