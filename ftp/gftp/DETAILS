   SOURCE_URL[1]=ftp://www.gftp.org/pub/gftp/$SOURCE
     SOURCE_HASH=sha512:59968786d66aaf6fb520f73d52ed0f8b57751a5ed222a5c65a7257628cd9addfd0ff3213206311bfd24441d7e6cc6d245d74d6687a1ad9739361da61ca078172
