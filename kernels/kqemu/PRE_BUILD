default_pre_build                                                             &&
cd "$SOURCE_DIRECTORY"                                                        &&
patch -Np1 < "$SCRIPT_DIRECTORY/kqemu-1.3.0pre11_linux-2.6.22.patch"          &&
sedit  "s:\`uname -r\`:$(get_kernel_version):"  $SOURCE_DIRECTORY/install.sh  &&
sedit  "s:depmod -a:& $(get_kernel_version):"   $SOURCE_DIRECTORY/install.sh
