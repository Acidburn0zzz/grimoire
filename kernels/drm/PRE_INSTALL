if [[ -d $INSTALL_ROOT/lib/modules/$(uname -r)/kernel/drivers/char/drm/ ]]
then
  cd $INSTALL_ROOT/lib/modules/$(uname -r)/kernel/drivers/char/drm/  &&
  local i                                                            &&
  for i in *.ko
  do
    mv -fv $i $i.bak
  done
fi  &&

default_pre_install
